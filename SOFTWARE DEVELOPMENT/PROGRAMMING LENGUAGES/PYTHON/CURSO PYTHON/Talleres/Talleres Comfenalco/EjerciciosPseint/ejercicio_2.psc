Algoritmo ejercicio_2
	Escribir "Inicio del programa";
	Escribir "Digite el nombre, edad y sexo de la persona 1"
	leer name, age, sex;
	Escribir "Digite el nombre, edad y sexo de la persona 1"
	leer name2, age2, sex2;
	Si age > age2 Entonces
		Escribir "La persona: ",name," es mayor que: ",name2,"tiene: ",age," y es de genero: ",sex;
	Sino
		Escribir "La persona: ",name2," es mayor que: ",name," tiene: ",age2," años y es de genero: ",sex2;
	FinSi
	Escribir "Fin del programa";
FinAlgoritmo
